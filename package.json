{
  "name": "flask-node",
  "version": "0.4.1",
  "description": "similar to python Flask web-framework",
  "main": "index.js",
  "files": [
    "docs",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "index.js"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register ./tests/*.ts",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.ts --report lcovonly --compilers ts:ts-node/register && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- tests/*.ts --report lcovonly --compilers ts:ts-node/register",
    "report": "mocha --recursive --reporter mochawesome",
    "clean": "rm -rf typings",
    "tsc": "tsc",
    "build": "npm run clean && npm run tsc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lleobox/nodeServer.git"
  },
  "keywords": [
    "flask",
    "webserver",
    "web-framework"
  ],
  "author": "lleohao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lleobox/nodeServer/issues"
  },
  "homepage": "https://github.com/lleobox/nodeServer#readme",
  "dependencies": {
    "formidable": "^1.0.17",
    "mime": "^1.3.4",
    "parseurl": "^1.3.1",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@types/formidable": "^1.0.29",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/should": "^8.1.30",
    "colors": "^1.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "docsify": "^3.0.4",
    "mocha": "^3.0.2",
    "mochawesome": "^1.5.2",
    "should": "^11.1.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
