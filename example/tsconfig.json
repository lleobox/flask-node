{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "experimentalDecorators": true,

        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,

        "sourceMap": true
    }
}