{
  "name": "flask-node",
  "version": "0.5.0",
  "description": "similar to python Flask web-framework",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib/* && tsc -p src",
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register ./test/*.ts",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- test/*.ts --report lcovonly --compilers ts:ts-node/register && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test/*.ts --report lcovonly --compilers ts:ts-node/register",
    "report": "mocha --recursive --reporter mochawesome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lleobox/nodeServer.git"
  },
  "keywords": [
    "flask",
    "webserver",
    "web-framework"
  ],
  "author": "lleohao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lleobox/nodeServer/issues"
  },
  "homepage": "https://github.com/lleobox/nodeServer#readme",
  "dependencies": {
    "formidable": "^1.1.1",
    "mime": "^1.3.4",
    "parseurl": "^1.3.1",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@types/formidable": "^1.0.29",
    "@types/mime": "^0.0.29",
    "@types/mocha": "^2.2.39",
    "@types/node": "<7.0.0",
    "@types/should": "^8.1.30",
    "@types/swig": "^0.0.28",
    "cz-conventional-changelog": "^1.2.0",
    "docsify": "^3.3.0",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.4",
    "rimraf": "^2.6.1",
    "should": "^11.2.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
